    "ip-upload": {
      "root": "apps/ip-upload/ip-upload/",
      "sourceRoot": "apps/ip-upload/ip-upload/src",
      "projectType": "application",
      "prefix": "ip",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ip-upload/ip-upload",
            "index": "apps/ip-upload/ip-upload/src/index.html",
            "main": "apps/ip-upload/ip-upload/src/main.ts",
            "polyfills": "apps/ip-upload/ip-upload/src/polyfills.ts",
            "tsConfig": "apps/ip-upload/ip-upload/tsconfig.app.json",
            "assets": [
              "apps/ip-upload/ip-upload/src/favicon.ico",
              "apps/ip-upload/ip-upload/src/assets",
              "apps/ip-upload/ip-upload/src/manifest.json"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/ip-upload/ip-upload/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "env/env.ts",
                  "with": "env/env.prod.ts"
                },
                {
                  "replace": "apps/ip-upload/ip-upload/src/environments/environment.ts",
                  "with": "apps/ip-upload/ip-upload/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "env/env.ts",
                  "with": "env/env.staging.ts"
                },
                {
                  "replace": "apps/ip-upload/ip-upload/src/environments/environment.ts",
                  "with": "apps/ip-upload/ip-upload/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ip-upload:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ip-upload:build:production"
            },
            "staging": {
              "browserTarget": "ip-upload:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ip-upload:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ip-upload/ip-upload/tsconfig.app.json",
              "apps/ip-upload/ip-upload/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/ip-upload/ip-upload/jest.config.js",
            "tsConfig": "apps/ip-upload/ip-upload/tsconfig.spec.json",
            "setupFile": "apps/ip-upload/ip-upload/src/test-setup.ts"
          }
        }
      }
    },
