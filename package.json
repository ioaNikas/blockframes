{
  "name": "blockframes",
  "version": "0.0.2",
  "license": "MIT",
  "main": "./main.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "ng": "ng",
    "start": "npx ng serve --hmr --disable-host-check",
    "build:stats": "ng build --prod --stats-json",
    "build:main": "npx ng build main --base-href / --configuration=\"${ENV:-dev}\"",
    "build:delivery": "npx ng build delivery --base-href /delivery/ --configuration=\"${ENV:-dev}\"",
    "build:movie-financing": "npx ng build movie-financing --base-href /movie-financing/ --configuration=\"${ENV:-dev}\"",
    "build:ip-upload": "npx ng build ip-upload --base-href /ip-upload/ --configuration=\"${ENV:-dev}\"",
    "build:catalog-marketplace": "npx ng build catalog-marketplace --base-href /catalog/ --configuration=\"${ENV:-dev}\"",
    "build:functions": "npx ng build backend-functions --configuration=\"${ENV:-dev}\"",
    "build:root": "rm -r ./dist/apps/assets && mv ./dist/apps/main/* ./dist/apps/",
    "build:all": "npm run build:main && npm run build:delivery && npm run build:catalog-marketplace && npm run build:movie-financing && npm run build:ip-upload && npm run build:functions && npm run build:root",
    "firebase:serve": "concurrently --kill-others \"npm run build:functions --watch\" \"firebase serve --only functions\"",
    "firebase:shell": "concurrently --kill-others \"npm run build:functions --watch\" \"firebase functions:shell\" --raw",
    "firebase:deploy": "firebase deploy",
    "test": "npx ng test",
    "lint": "npx ng lint",
    "e2e": "npm run e2e:main && npm run e2e:ip-upload && npm run e2e:delivery && npm run e2e:catalog && npm run e2e:movie-financing",
    "pree2e": "ng build backend-ops --configuration=\"${ENV}\" && node dist/apps/backend-ops/main.js",
    "e2e:main": "./node_modules/.bin/ng e2e main-e2e --configuration=\"${ENV}\" --headless",
    "e2e:ip-upload": "./node_modules/.bin/ng e2e ip-upload-e2e --configuration=\"${ENV}\" --headless",
    "e2e:delivery": "./node_modules/.bin/ng e2e delivery-e2e --configuration=\"${ENV}\" --headless",
    "e2e:catalog": "./node_modules/.bin/ng e2e catalog-e2e --configuration=\"${ENV}\" --headless",
    "e2e:movie-financing": "./node_modules/.bin/ng e2e movie-financing-e2e --configuration=\"${ENV}\" --headless",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "contracts:build": "npx waffle ./waffle-config.json",
    "affected": "./node_modules/.bin/nx affected",
    "analyze:delivery": "webpack-bundle-analyzer dist/apps/delivery/stats.json --port=8000"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.3",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.1.3",
    "@angular/compiler": "^8.1.3",
    "@angular/core": "^8.1.3",
    "@angular/fire": "^5.1.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.1.3",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "^8.1.3",
    "@angular/platform-browser-dynamic": "^8.1.3",
    "@angular/router": "^8.1.3",
    "@angular/service-worker": "^8.1.3",
    "@datorama/akita": "^3.8.3",
    "@fullpage/angular-fullpage": "0.0.11",
    "@google-cloud/pubsub": "^0.24.1",
    "@google-cloud/storage": "^2.4.2",
    "@ngrx/effects": "8.1.0",
    "@ngrx/router-store": "8.1.0",
    "@ngrx/store": "8.1.0",
    "@nrwl/angular": "8.4.1",
    "@schematics/angular": "^8.1.3",
    "@ngmodule/material-carousel": "^0.5.0",
    "@sendgrid/mail": "^6.4.0",
    "@sentry/browser": "^5.5.0",
    "@sentry/node": "^5.5.0",
    "@types/sendgrid": "^4.3.0",
    "algoliasearch": "^3.33.0",
    "core-js": "^2.6.4",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^4.0.27",
    "express": "4.16.4",
    "firebase": "^5.8.2",
    "firebase-admin": "^8.2.0",
    "firebase-functions": "^3.0.2",
    "generate-password": "^1.4.2",
    "hammerjs": "^2.0.8",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.11",
    "ngx-mat-select-search": "^1.7.2",
    "node-gyp": "^4.0.0",
    "npm": "^6.9.0",
    "pdfmake": "^0.1.57",
    "punycode": "^2.1.1",
    "request": "^2.88.0",
    "rxjs": "~6.5.2",
    "tmp": "0.0.33",
    "xlsx": "^0.14.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.801.1",
    "@angular-devkit/build-ng-packagr": "~0.801.1",
    "@angular/cli": "8.1.1",
    "@angular/compiler-cli": "^8.1.3",
    "@angular/language-service": "^8.1.3",
    "@angularclass/hmr": "^2.1.3",
    "@datorama/akita-ng-router-store": "^3.0.2",
    "@datorama/akita-ngdevtools": "^1.0.4",
    "@ngrx/schematics": "8.1.0",
    "@ngrx/store-devtools": "8.1.0",
    "@nrwl/cypress": "8.4.1",
    "@nrwl/express": "8.4.1",
    "@nrwl/jest": "8.4.1",
    "@nrwl/node": "8.4.1",
    "@nrwl/workspace": "8.4.1",
    "@types/algoliasearch": "^3.30.16",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.9",
    "@types/jquery": "3.3.29",
    "@types/line-reader": "0.0.28",
    "@types/lodash": "^4.14.123",
    "@types/node": "~10.12.23",
    "@types/pdfmake": "^0.1.7",
    "@types/tmp": "0.0.34",
    "akita-schematics": "^2.0.0",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "cypress": "^3.1.5",
    "dotenv": "6.2.0",
    "ethereum-waffle": "^2.0.9",
    "jest": "24.1.0",
    "jest-preset-angular": "7.0.0",
    "ng-packagr": "^5.2.0",
    "openzeppelin-solidity": "^2.2.0",
    "pdfkit": "^0.9.0",
    "prettier": "1.15.2",
    "tmp": "0.0.33",
    "ts-node": "~8.0.2",
    "tsickle": "0.34.3",
    "tslib": "^1.9.0",
    "tslint": "~5.12.1",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.4.1"
  }
}
